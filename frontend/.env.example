# MedVaani Frontend Environment Variables

# Backend API Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_WS_URL=ws://localhost:8000/ws/health

# Development Configuration
REACT_APP_ENV=development

# Feature Flags
REACT_APP_ENABLE_VOICE_RECORDING=true
REACT_APP_ENABLE_ENHANCED_CONSULTATION=true
REACT_APP_ENABLE_HEALTH_MONITORING=true

# Analytics (optional)
REACT_APP_ENABLE_ANALYTICS=false

# Error Tracking (optional)
REACT_APP_ENABLE_ERROR_TRACKING=false
